{
  "extends": ["next/core-web-vitals","plugin:tailwindcss/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false,
        "semi": true,
        "printWidth": 120,
        "trailingComma": "none"
      }
    ]
  }
}
